<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/buttonGrayedPressed"/>
            <corners android:radius="0dp"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/buttonGrayedDisabled"/>
            <corners android:radius="0dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/buttonGrayedPrimary"/>
            <corners android:radius="0dp"/>
        </shape>
    </item>
</selector>